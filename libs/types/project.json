{
  "projectType": "library",
  "root": "libs/types",
  "sourceRoot": "libs/types/src",
  "prefix": "playlist-showcase",
  "targets": {
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": [
        "coverage/libs/types"
      ],
      "options": {
        "jestConfig": "libs/types/jest.config.js",
        "passWithNoTests": true
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": [
          "libs/types/src/**/*.ts",
          "libs/types/src/**/*.html"
        ]
      }
    }
  },
  "tags": []
}
